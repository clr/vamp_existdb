    <rdf:li>Messier 74</rdf:li>
    <rdf:li>M74</rdf:li>
    <rdf:li>NGC 628</rdf:li>
    <rdf:li>Supernova 2003gd</rdf:li>
   </rdf:Bag>
  </dc:subject>
