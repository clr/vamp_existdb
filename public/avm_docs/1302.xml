  </dc:subject>
  <dc:title>
   <rdf:Alt>
    <rdf:li xml:lang='x-default'>Spitzer Digs Up Galactic Fossil</rdf:li>
   </rdf:Alt>
  </dc:title>
 </rdf:Description>

