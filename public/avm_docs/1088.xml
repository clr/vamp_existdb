   </rdf:Bag>
  </dc:subject>
  <dc:title>
   <rdf:Alt>
    <rdf:li xml:lang='x-default'>Embedded Outflow in HH 46/47</rdf:li>
   </rdf:Alt>
  </dc:title>
 </rdf:Description>

