   </rdf:Bag>
  </dc:subject>
  <dc:title>
   <rdf:Alt>
    <rdf:li xml:lang='x-default'>Spitzer View of the Core L1014</rdf:li>
   </rdf:Alt>
  </dc:title>
 </rdf:Description>

