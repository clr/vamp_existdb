  <dc:title>
   <rdf:Alt>
    <rdf:li xml:lang='x-default'>&#39;Peony Nebula&#39; Star Settles for Silver Medal</rdf:li>
   </rdf:Alt>
  </dc:title>
 </rdf:Description>

